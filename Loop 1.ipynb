   "source": [
    "n = int(input(\"Enter number of terms in the fibonacci series: \"))\n",
    "a, b  = 0, 1 #where a & b are the 1st & 2nd terms\n",
    "i = 0 #loop variable\n",
    "print(\"Fibonacci series upto \",n,\" number of terms: \")\n",
    "while i < n:\n",
    "    print(a)\n",
    "    c = a + b\n",
    "    a = b\n",
    "    b = c\n",
    "    i+=1"
   ]
  }
